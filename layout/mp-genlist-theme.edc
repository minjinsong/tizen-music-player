#define TEST_RECT(test)\
	part { name: "test_rect";\
            type: RECT; \
            description { state: "default" 0.0; \
               color: 255 0 0 255; \
               rel1 {relative: 0.0 0.0; to, test;}\
               rel2 {relative: 1.0 1.0; to, test;}\
               }\
         }

#define TEST_RECT2(test)\
	part { name: "test_rect2";\
            type: RECT; \
            description { state: "default" 0.0; \
               color: 255 255 0 255; \
               rel1 {relative: 0.0 0.0; to, test;}\
               rel2 {relative: 1.0 1.0; to, test;}\
               }\
         }

#define TEST_RECT3(test)\
	part { name: "test_rect3";\
            type: RECT; \
            description { state: "default" 0.0; \
               color: 255 0 255 255; \
               rel1 {relative: 0.0 0.0; to, test;}\
               rel2 {relative: 1.0 1.0; to, test;}\
               }\
         }

